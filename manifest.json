{
    "manifest_version": 3,
    "name": "Bookmarks+",
    "description": "Save and sort links, images, and text from across the web",
    "version": "1.0", 
    "permissions": [
        "tabs", 
        "contextMenus",  
        "storage"
    ],
    "icons": {
        "16": "save-16.png",
        "48": "save-48.png",
        "128": "save-128.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": "save-48.png"
    }, 
    "background": {
        "service_worker": "background.js"
    }
   

}